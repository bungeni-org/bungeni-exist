        <acl axis="bu:document[@type=$$TYPE] and bu:legislativeItem/(bu:permissions except bu:versions)/bu:permission" name="public-view">
            <permission name="zope.View" role="bungeni.Anonymous" setting="Allow"/>
        </acl>
        <acl axis="following-sibling::bu:permisssions/bu:permission" condition="@name='zope.View' and @role='bungeni.Authenticated' and @setting='Allow'" name="authenticated-view"/>
    </acl-groups>
    <doctypes>
        <doctype name="userdata">
            <orderbys>
                <orderby order="asc" value="st_date_oldest">status date [oldest]</orderby>
                <orderby order="desc" value="st_date_newest">status date [newest]</orderby>
            </orderbys>
            <searchins>
                <searchin field="." name="all" value="all">Entire Document</searchin>
                <searchin default="true" field="bu:user/bu:firstName" name="fn" value="fstname">First Name</searchin>
                <searchin field="bu:user/bu:lastName" name="ln" value="lsname">Last Name</searchin>
                <searchin field="bu:user/bu:description" name="ds" value="desc">Description</searchin>
            </searchins>
        </doctype>
        <doctype name="committee">
            <orderbys>
                <orderby order="desc" value="st_date_oldest">status date [oldest]</orderby>
                <orderby order="asc" value="st_date_newest">status date [newest]</orderby>
            </orderbys>
            <searchins>
                <searchin field="." name="all" value="all">Entire Document</searchin>
                <searchin default="true" field="bu:legislature/bu:fullName" name="fn" value="fname">Full Name</searchin>
                <searchin field="bu:legislature/bu:shortName" name="sn" value="sname">Short Name</searchin>
                <searchin field="bu:legislature/bu:description" name="ds" value="desc">Description</searchin>
            </searchins>
        </doctype>
        <doctype name="political-group">
            <orderbys>
                <orderby order="asc" value="st_date_oldest">status date [oldest]</orderby>
                <orderby order="desc" value="st_date_newest">status date [newest]</orderby>
            </orderbys>
            <searchins>
                <searchin field="." name="all" value="all">Entire Document</searchin>
                <searchin default="true" field="bu:legislature/bu:fullName" name="fn" value="fname">Full Name</searchin>
                <searchin field="bu:legislature/bu:shortName" name="sn" value="sname">Short Name</searchin>
                <searchin field="bu:legislature/bu:description" name="ds" value="desc">Description</searchin>
            </searchins>
        </doctype>
        <doctype name="bill">
            <orderbys>
                <orderby order="asc" value="st_date_oldest">status date [oldest]</orderby>
                <orderby order="desc" value="st_date_newest">status date [newest]</orderby>
            </orderbys>
            <searchins>
                <searchin field="." name="all" value="all">Entire Document</searchin>
                <searchin default="true" field="bu:legislativeItem/bu:shortName" name="f_t" value="title">Title</searchin>
                <searchin field="bu:legislativeItem/bu:body" name="f_b" value="body">Body</searchin>
                <searchin field="bu:legislativeItem/bu:registryNumber" name="f_d" value="docno">Doc No.</searchin>
            </searchins>
        </doctype>
        <doctype name="question">
